                      "attributes_abilities": {"module": "the_tale.game.heroes.habilities.attributes"},
                      "companions_abilities": {"module": "the_tale.game.heroes.habilities.companions"},
                      "nonbattle_abilities": {"module": "the_tale.game.heroes.habilities.nonbattle"},

                      "ABILITIES": {"module": "the_tale.game.heroes.habilities",
                                    "attribute": "ABILITIES"},
                      "ABILITY_AVAILABILITY": {"module": "the_tale.game.heroes.habilities",
                                               "attribute": "ABILITY_AVAILABILITY"},

                      "cards_types": {"module": "the_tale.game.cards.cards"},

                      "linguistics_helpers": {"module": "the_tale.linguistics.tests.helpers"},
                      "blogs_helpers": {"module": "the_tale.blogs.tests.helpers"}

                  }
              },
              "rule_stdlib": {}}
}
